{
	"name": "counterexample",
	"version": "1.0.0",
	"description": "Report gererated by the CPAchecker",
	"author": "",
	"license": "ISC",
	"private": true,
	"scripts": {
		"inject-workers": "node scripts/injectWorkers.js",
		"check-dependencies": "node scripts/checkDependencies.js",
		"build": "npm run check-dependencies && npm run inject-workers && webpack",
		"start": "npm run build & webpack serve --mode=development",
		"test": "karma start karma.conf.js",
		"e2e-test": "protractor e2e-conf.js",
		"lint:fix": "eslint --fix --ignore-path .lintignores $(pwd)'/**/*.js' && prettier --ignore-path .lintignores --write $(pwd)'/**/*.+(css|json)'",
		"lint": "eslint --max-warnings 0 --ignore-path .lintignores . && prettier --ignore-path .lintignores --check $(pwd)'/**/*.+(css|json)'"
	},
	"devDependencies": {
		"@webpack-cli/serve": "^1.3.1",
		"angular-mocks": "^1.8.2",
		"code-prettify": "^0.1.0",
		"css-loader": "^5.2.2",
		"css-minimizer-webpack-plugin": "^1.1.5",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-prettier": "^3.4.0",
		"is-docker": "^2.2.1",
		"jasmine": "^3.7.0",
		"jasmine-jquery": "^2.1.1",
		"karma": "^6.3.2",
		"karma-chrome-launcher": "^3.1.0",
		"karma-cli": "^2.0.0",
		"karma-detect-browsers": "^2.3.3",
		"karma-firefox-launcher": "^2.1.0",
		"karma-htmlfile-reporter": "^0.3.8",
		"karma-jasmine": "^4.0.1",
		"karma-jquery": "^0.2.4",
		"karma-webpack": "^5.0.0",
		"license-checker": "^25.0.1",
		"mini-css-extract-plugin": "^1.4.1",
		"prettier": "^2.2.1",
		"protractor": "^7.0.0",
		"protractor-jasmine2-screenshot-reporter": "^0.5.0",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.2",
		"webpack-cli": "^4.3.0",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.3",
		"angular": "^1.8.1",
		"angular-sanitize": "^1.8.2",
		"bootstrap": "^4.6.0",
		"datatables.net": "^1.10.24",
		"datatables.net-dt": "^1.10.24",
		"jquery": "^3.6.0",
		"jquery-resizable-dom": "^0.35.0",
		"popper.js": "^1.16.1"
	},
	"eslintConfig": {
		"extends": [
			"plugin:prettier/recommended"
		],
		"env": {
			"browser": true,
			"es2021": true
		},
		"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
		},
		"plugins": [
			"prettier"
		],
		"rules": {
			"prettier/prettier": "error",
			"require-jsdoc": 0
		}
	}
}
